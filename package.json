  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "mocha": "^3.5.1",
    "rimraf": "^2.6.1",
    "semistandard": "^11.0.0",
    "sinon": "^3.2.1"
